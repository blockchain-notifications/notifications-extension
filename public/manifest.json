{
  "name": "Notify Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "notifications",
    "storage"
  ],
  "icons": {
    "16": "notification.png"
  },
  "background": {
    "scripts": [
      "watchNotifications.js"
    ]
  }
}